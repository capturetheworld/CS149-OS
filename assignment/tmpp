void startPipe(int l_fd1[2], int l_fd2[2], int l_fd3[2], int l_fd4[2]){
 	if (pipe(l_fd1) == -1){
	 	fprintf(stderr,"Pipe Failed");
		exit(EXIT_FAILURE);
	}
 	if (pipe(l_fd2) == -1){
	 	fprintf(stderr,"Pipe Failed");
		exit(EXIT_FAILURE);
	}
 	if (pipe(l_fd3) == -1){
	 	fprintf(stderr,"Pipe Failed");
		exit(EXIT_FAILURE);
	}
 	if (pipe(l_fd4) == -1){
	 	fprintf(stderr,"Pipe Failed");
		exit(EXIT_FAILURE);
	}

}

